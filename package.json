{
    "name": "@webby-crown/strapi-api-deep-populate",
    "version": "1.0.0",
    "description": "Auto-populate images, relations, and nested components based on settings",
    "private": false,
  
    "files": [
      "dist/",
      "strapi-admin.js",
      "strapi-server.js",
      "package.json",
      "README.md"
    ],
  
    "scripts": {
      "clean": "rimraf dist",
      "build:admin": "webpack --config webpack.config.js",
      "build:server": "webpack --config webpack.server.config.js",
      "build": "npm run clean && npm run build:admin && npm run build:server",
      "prepublishOnly": "npm run build"
    },
  
    "keywords": [
      "strapi",
      "strapi-plugin",
      "deep-populate",
      "populate",
      "relations",
      "components",
      "media"
    ],
  
    "author": "Webby Crown",
    "license": "MIT",
  
    "repository": {
      "type": "git",
      "url": "https://github.com/Webby-Crown/strapi-api-deep-populate.git"
    },
  
    "bugs": {
      "url": "https://github.com/Webby-Crown/strapi-api-deep-populate/issues"
    },
  
    "homepage": "https://github.com/Webby-Crown/strapi-api-deep-populate#readme",
  
    "peerDependencies": {
      "@strapi/strapi": "^5.0.0"
    },
  
    "devDependencies": {
      "@babel/core": "^7.22.0",
      "@babel/preset-env": "^7.22.0",
      "@babel/preset-react": "^7.22.0",
      "babel-loader": "^9.1.0",
      "rimraf": "^5.0.0",
      "webpack": "^5.88.0",
      "webpack-cli": "^5.1.0"
    },
  
    "strapi": {
      "name": "strapi-api-deep-populate",
      "displayName": "API Deep Populate",
      "description": "Auto-populate images, relations, and nested components based on settings",
      "kind": "plugin",
      "admin": "strapi-admin.js",
      "server": "strapi-server.js"
    }
  }
  